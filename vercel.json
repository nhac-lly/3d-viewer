{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, s-maxage=31536000, stale-while-revalidate=31536000, stale-if-error=31536000"
                }
            ]
        }
    ]
}